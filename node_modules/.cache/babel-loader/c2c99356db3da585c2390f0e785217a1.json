;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qBACA,KAAA,CAAA,MAAA,CAAA,QAAA,CAAA,iBAAA,CADA;;AAAA;AAEA,cAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,MAAA,CAAA,MAAA,CAAA,SAAA;;AAFA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAGA,GAnEA;AAoEA,EAAA,KAAA,EAAA;AACA,IAAA,IADA,kBACA;AACA,WAAA,aAAA,GAAA,KAAA,UAAA,CAAA,KAAA,IAAA,CAAA;AACA;AAHA,GApEA;AAyEA,EAAA,OAAA,EAAA;AACA,IAAA,WADA,uBACA,IADA,EACA;AACA,WAAA,OAAA,CAAA,IAAA,yBAAA,IAAA,CAAA,EAAA;AACA,KAHA;AAIA,IAAA,YAJA,wBAIA,IAJA,EAIA;AACA,WAAA,UAAA,GAAA,IAAA;AACA,WAAA,cAAA,GAAA,IAAA,CAAA,SAAA;AACA,KAPA;AAQA,IAAA,UARA,sBAQA,IARA,EAQA;AACA,UAAA,CAAA,IAAA,EAAA,OAAA,IAAA;;AADA,wBAGA,IAAA,CAAA,KAAA,CAAA,GAAA,CAHA;AAAA;AAAA,UAGA,IAHA;AAAA,UAGA,KAHA;AAAA,UAGA,GAHA;;AAIA,uBAAA,IAAA,cAAA,KAAA,cAAA,GAAA;AACA,KAbA;AAcA,IAAA,SAdA,qBAcA,IAdA,EAcA;AACA,UAAA,CAAA,IAAA,EAAA,OAAA,IAAA;;AADA,yBAGA,IAAA,CAAA,KAAA,CAAA,GAAA,CAHA;AAAA;AAAA,UAGA,KAHA;AAAA,UAGA,GAHA;AAAA,UAGA,IAHA;;AAIA,uBAAA,IAAA,cAAA,KAAA,CAAA,QAAA,CAAA,CAAA,EAAA,GAAA,CAAA,cAAA,GAAA,CAAA,QAAA,CAAA,CAAA,EAAA,GAAA,CAAA;AACA,KAnBA;AAoBA,IAAA,MApBA,kBAoBA,IApBA,EAoBA;AACA,UAAA,KAAA,GAAA,KAAA,OAAA,CAAA,OAAA,CAAA,IAAA,CAAA,IAAA,CAAA;AACA,UAAA,KAAA,IAAA,CAAA,EAAA,KAAA,OAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;AACA,KAvBA;AAwBA,IAAA,SAxBA,qBAwBA,IAxBA,EAwBA;AACA,UAAA,KAAA,GAAA,KAAA,IAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,UAAA,KAAA,IAAA,CAAA,EAAA,KAAA,IAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;AACA;AA3BA;AAzEA,CAAA","sourcesContent":["<template>\n  <div style=\"flex:1; flex-direction:column;\" class=\"d-flex\">\n    <div class=\"d-flex pa-4\" style=\"height:54px;align-items: center;\">\n      <div class=\"mr-4 font-weight-medium\">專業設定</div>\n      <v-divider class=\"mr-4\" vertical></v-divider>\n      <v-breadcrumbs :items=\"breadcrumbItems\" class=\"pa-0\">\n        <template v-slot:divider>\n          —\n        </template>\n      </v-breadcrumbs>\n      <v-spacer></v-spacer>\n      <v-btn depressed class=\"btn-back\" @click=\"$router.go(-1)\"\n        >返回</v-btn\n      >\n    </div>\n    <div\n      style=\"background: #FAFCFE;overflow: scroll; flex:1; flex-direction:column;\"\n      class=\"pa-4 d-flex\"\n    >\n      <h2 class=\"mb-4\">建立新專案</h2>\n      <div\n        style=\"border-radius:12px; background: #ffffff\"\n        class=\"px-8 py-5 mb-4\"\n      >\n        <h3 class=\"mb-8\">專案設定</h3>\n        <!-- <div class=\"d-flex align-center mb-4\"> -->\n        <v-row class=\"align-center\">\n          <v-col class=\"col-1 pa-0\">\n            <!-- <div style=\"width: 50%\" class=\"d-flex align-center\"> -->\n            <span class=\"font-weight-medium\">關鍵字</span>\n          </v-col>\n          <v-col class=\"pa-0\">\n            <v-text-field\n              placeholder=\"輸入文字內容\"\n              outlined\n              dense\n              hide-details=\"true\"\n              class=\"mr-4\"\n            ></v-text-field>\n          </v-col>\n          <!-- </div> -->\n          <!-- <div style=\"width: 50%\" class=\"d-flex align-center\"> -->\n          <v-col class=\"col-1 pa-0\">\n            <span class=\"font-weight-medium\">專案狀態</span>\n          </v-col>\n          <v-col class=\"pa-0\">\n            <v-radio-group v-model=\"row\" row>\n              <v-radio label=\"公開\" value=\"radio-1\"></v-radio>\n              <v-radio label=\"私人\" value=\"radio-2\"></v-radio>\n            </v-radio-group>\n          </v-col>\n        </v-row>\n        <!-- </div> -->\n        <!-- </div> -->\n        <!-- <div class=\"d-flex align-center mb-4\">\n                    <div class=\"d-flex align-center mb-4\" style=\"width: 50%\"> -->\n        <v-row class=\"align-center\">\n          <v-col class=\"col-1 pa-0\">\n            <div class=\"font-weight-medium\">標記者</div>\n          </v-col>\n          <v-col class=\"pa-0\">\n            <v-autocomplete\n              v-model=\"friends\"\n              :items=\"people\"\n              chips\n              item-text=\"name\"\n              item-value=\"name\"\n              multiple\n              outlined\n              dense\n              hide-details=\"true\"\n              class=\"pr-4\"\n            >\n              <template v-slot:selection=\"data\">\n                <v-chip\n                  v-bind=\"data.attrs\"\n                  :input-value=\"data.selected\"\n                  close\n                  @click=\"data.select\"\n                  @click:close=\"remove(data.item)\"\n                  class=\"mb-1\"\n                >\n                  <v-avatar left>\n                    <v-img :src=\"data.item.avatar\"></v-img>\n                  </v-avatar>\n                  {{ data.item.name }}\n                </v-chip>\n              </template>\n              <!-- <template v-slot:item=\"data\">\n                            <template v-if=\"typeof data.item !== 'object'\">\n                                <v-list-item-content>{{data.item}}</v-list-item-content>\n                            </template>\n                            <template v-else>\n                            <v-list-item-avatar>\n                            </v-list-item-avatar>\n                            <v-list-item-content>\n                                <v-list-item-title>{{data.item.name}}</v-list-item-title>\n                                <v-list-item-subtitle>{{data.item.group}}</v-list-item-subtitle>\n                            </v-list-item-content>\n                            </template>\n                        </template> -->\n            </v-autocomplete>\n          </v-col>\n          <v-col class=\"col-1 pa-0\">\n            <span class=\"font-weight-medium\">標籤</span>\n          </v-col>\n          <v-col class=\"pa-0\">\n            <v-combobox\n              v-model=\"tags\"\n              multiple\n              chips\n              outlined\n              dense\n              hide-details=\"true\"\n            >\n              <template v-slot:selection=\"data\">\n                <v-chip\n                  v-bind=\"data.attrs\"\n                  :input-value=\"data.selected\"\n                  @click:close=\"removeTag(data.item)\"\n                  @click=\"data.select\"\n                  close\n                  class=\"mb-1\"\n                >\n                  <span class=\"pr-2\">\n                    {{ data.item }}\n                  </span>\n                </v-chip>\n              </template>\n            </v-combobox>\n          </v-col>\n        </v-row>\n        <div class=\"d-flex mt-8\">\n          <v-spacer></v-spacer>\n          <v-btn outlined class=\"mr-4 btn-outlined\">清除</v-btn>\n          <v-btn depressed class=\"btn-depressed\">確定</v-btn>\n        </div>\n      </div>\n      <div style=\"border-radius:12px; background: #ffffff\" class=\"px-8 py-5\">\n        <div style=\"background: #F5F8FA; border-radius:4px\" class=\"pa-4 mb-4\">\n          <h3>加入判決書</h3>\n          <div class=\"mb-4 px-4\">\n            <v-row class=\"align-center\">\n              <v-col class=\"col-1 pa-0\">\n                <span class=\"font-weight-medium\">判決類型</span>\n              </v-col>\n              <v-col>\n                <v-radio-group v-model=\"row\" row>\n                  <v-radio label=\"民事\" value=\"radio-1\"></v-radio>\n                  <v-radio label=\"刑事\" value=\"radio-2\"></v-radio>\n                  <v-radio label=\"行政\" value=\"radio-3\"></v-radio>\n                  <v-radio label=\"憲法\" value=\"radio-4\"></v-radio>\n                  <v-radio label=\"懲戒\" value=\"radio-5\"></v-radio>\n                </v-radio-group>\n              </v-col>\n              <v-col class=\"col-1 pa-0\">\n                <span class=\"font-weight-medium\">裁判時間</span>\n              </v-col>\n              <v-col class=\"pa-0\">\n                <v-row class=\"align-center\">\n                  <v-col class=\"pa-0 pr-1\">\n                    <v-menu\n                      v-model=\"menu2\"\n                      :close-on-content-click=\"false\"\n                      transition=\"scale-transition\"\n                      offset-y\n                      max-width=\"290px\"\n                      min-width=\"auto\"\n                    >\n                      <template v-slot:activator=\"{ on, attrs }\">\n                        <v-text-field\n                          hide-details=\"true\"\n                          dense\n                          outlined\n                          placeholder=\"起始時間\"\n                          v-model=\"computedDateFormatted\"\n                          v-bind=\"attrs\"\n                          v-on=\"on\"\n                          append-icon=\"mdi-calendar\"\n                        ></v-text-field>\n                      </template>\n                      <v-date-picker\n                        v-model=\"date\"\n                        no-title\n                        @input=\"menu2 = false\"\n                      ></v-date-picker>\n                    </v-menu>\n                  </v-col>\n                  至\n                  <v-col class=\"pa-0 pl-1\">\n                    <v-menu\n                      v-model=\"menu2\"\n                      :close-on-content-click=\"false\"\n                      transition=\"scale-transition\"\n                      offset-y\n                      max-width=\"290px\"\n                      min-width=\"auto\"\n                    >\n                      <template v-slot:activator=\"{ on, attrs }\">\n                        <v-text-field\n                          hide-details=\"true\"\n                          dense\n                          outlined\n                          placeholder=\"結束時間\"\n                          v-model=\"computedDateFormatted\"\n                          v-bind=\"attrs\"\n                          v-on=\"on\"\n                          append-icon=\"mdi-calendar\"\n                        ></v-text-field>\n                      </template>\n                      <v-date-picker\n                        v-model=\"date\"\n                        no-title\n                        @input=\"menu2 = false\"\n                      ></v-date-picker>\n                    </v-menu>\n                  </v-col>\n                </v-row>\n              </v-col>\n            </v-row>\n            <v-row class=\"align-center\">\n              <v-col class=\"col-1 pa-0\">\n                <span class=\"font-weight-medium\">法院類別</span>\n              </v-col>\n              <v-col class=\"pa-0 col-5\">\n                <v-row class=\"mr-0 pa-1\">\n                  <v-col class=\"pa-0 pr-1\">\n                    <v-select\n                      dense\n                      outlined\n                      hide-details=\"true\"\n                      placeholder=\"地方法院\"\n                    ></v-select>\n                  </v-col>\n                  <v-col class=\"pa-0\">\n                    <v-select\n                      dense\n                      outlined\n                      hide-details=\"true\"\n                      placeholder=\"所有區域\"\n                    ></v-select>\n                  </v-col>\n                </v-row>\n              </v-col>\n              <v-col class=\"col-1 pa-0 pl-3\">\n                <span class=\"font-weight-medium\">\n                    關鍵字\n                   \n                </span>\n                 \n              </v-col>\n              <v-col class=\"pa-0 col-5\" style=\"display: contents;\">\n                <v-text-field\n                  placeholder=\"請輸入判決書名稱\"\n                  outlined\n                  dense\n                  hide-details=\"true\"\n                  class=\"mr-1\"\n                ></v-text-field>\n                <v-tooltip bottom >\n                    <template v-slot:activator=\"{ on, attrs }\">\n                        <v-icon\n                        v-bind=\"attrs\"\n                        v-on=\"on\"\n                        >\n                        mdi-information-slab-circle\n                        </v-icon>\n                    </template>\n                    <span v-html=\"tips\"></span>\n                </v-tooltip>\n              </v-col>\n            </v-row>\n          </div>\n          <div class=\"d-flex align-center mt-8\">\n            <v-spacer></v-spacer>\n            <v-btn depressed class=\"mr-4 btn-depressed\">搜尋</v-btn>\n            <v-btn outlined class=\"btn-outlined\">清除</v-btn>\n          </div>\n        </div>\n        <v-data-table\n          v-model=\"selected\"\n          :headers=\"headers\"\n          :items=\"judgmentList\"\n          :page.sync=\"page\"\n          :single-select=\"singleSelect\"\n          hide-default-footer\n          class=\"elevation-0 project_table\"\n          show-select\n          @page-count=\"pageCount = $event\"\n        >\n          <!-- <template v-slot:item.=\"{ item }\">\n            <span class=\"single-line-ellipsis\">{{ item.Court }}</span>\n          </template> -->\n          <template v-slot:item.Court=\"{ item }\">\n            <div>{{ item.Court }}</div>\n          </template>\n          <template v-slot:item.Judgement=\"{ item }\">\n            <span class=\"two-line-ellipsis\">{{ item.Judgement }}</span>\n          </template>\n          <template v-slot:item.actions=\"{ item }\">\n            <v-btn\n              depressed\n              style=\"background: #A1A5B7;color:#ffffff\"\n              @click=\"viewJudgment(item)\"\n              >查看</v-btn\n            >\n          </template>\n        </v-data-table>\n        <div class=\"d-flex mt-4 align-center\">\n          <span>Showing 1 to 10 of {{judgmentList.length}} entries</span>\n          <v-spacer></v-spacer>\n          <v-pagination\n            color=\"#53BBB2\"\n            v-model=\"page\"\n            :length=\"pageCount\"\n          ></v-pagination>\n        </div>\n        <div class=\"d-flex mt-8\">\n          <v-btn depressed class=\"mr-4 btn-depressed\">確定</v-btn>\n          <v-btn outlined class=\"btn-outlined\">清除</v-btn>\n        </div>\n      </div>\n    </div>\n    <v-dialog v-model=\"openDialog\" width=\"500\">\n      <v-card>\n        <v-card-text class=\"py-10\" v-html=\"judgmentString\"> </v-card-text>\n      </v-card>\n    </v-dialog>\n  </div>\n</template>\n<script>\n  import { mapGetters } from \"vuex\";\n\n  export default {\n    data() {\n      const srcs = {\n        1: \"https://cdn.vuetifyjs.com/images/lists/1.jpg\",\n        2: \"https://cdn.vuetifyjs.com/images/lists/2.jpg\",\n        3: \"https://cdn.vuetifyjs.com/images/lists/3.jpg\",\n        4: \"https://cdn.vuetifyjs.com/images/lists/4.jpg\",\n        5: \"https://cdn.vuetifyjs.com/images/lists/5.jpg\",\n      };\n      return {\n        tips: \"搜尋關鍵字用「空白鍵」或「&」連接代表and，用「+」連接代表or，用「-」<br/>連接在關鍵字前方代表不包含此關鍵字，需注意「-」前必須要用空白鍵隔開。\",\n        people: [\n          { header: \"Group 1\" },\n          { name: \"Sandra Adams\", group: \"Group 1\", avatar: srcs[1] },\n          { name: \"Ali Connors\", group: \"Group 1\", avatar: srcs[2] },\n          { name: \"Trevor Hansen\", group: \"Group 1\", avatar: srcs[3] },\n          { name: \"Tucker Smith\", group: \"Group 1\", avatar: srcs[2] },\n          { divider: true },\n          { header: \"Group 2\" },\n          { name: \"Britta Holt\", group: \"Group 2\", avatar: srcs[4] },\n          { name: \"Jane Smith \", group: \"Group 2\", avatar: srcs[5] },\n          { name: \"John Smith\", group: \"Group 2\", avatar: srcs[1] },\n          { name: \"Sandra Williams\", group: \"Group 2\", avatar: srcs[3] },\n        ],\n        tags: [],\n        friends: [\"Sandra Adams\", \"Britta Holt\"],\n        openDialog: false,\n        judgmentString: \"\",\n        selected: [],\n        singleSelect: false,\n        pageCount: 10,\n        page: 1,\n        dateFormatted: null,\n        date: \"\",\n        menu2: false,\n        headers: [\n          {\n            text: \"裁判字號\",\n            align: \"start\",\n            sortable: false,\n            value: \"Court\",\n          },\n          { text: \"判決時間\", sortable: false, value: \"Date\", width: \"120px\" },\n          { text: \"法院層級\", sortable: false, value: \"Court\", width: \"100px\" },\n          { text: \"內容預覽\", sortable: false, value: \"Judgement\", width: \"40%\" },\n          { text: \"操作\", sortable: false, value: \"actions\", align: \"center\" },\n        ],\n        breadcrumbItems: [\n          {\n            text: \"我的專案\",\n            disabled: true,\n          },\n        ],\n        row: null,\n      };\n    },\n    computed: {\n      computedDateFormatted() {\n        return this.formatDate(this.date);\n      },\n      ...mapGetters({\n        judgmentList: \"judgmentList\",\n      }),\n    },\n    async mounted() {\n      await this.$store.dispatch(\"getJudgmentList\");\n      console.log(this.$route.params.projectId)\n    },\n    watch: {\n      date() {\n        this.dateFormatted = this.formatDate(this.date);\n      },\n    },\n    methods: {\n      markSetting(item) {\n        this.$router.push(`/mark-setting/${item.id}`);\n      },\n      viewJudgment(item) {\n        this.openDialog = true;\n        this.judgmentString = item.Judgement;\n      },\n      formatDate(date) {\n        if (!date) return null;\n\n        const [year, month, day] = date.split(\"-\");\n        return `${year}/${month}/${day}`;\n      },\n      parseDate(date) {\n        if (!date) return null;\n\n        const [month, day, year] = date.split(\"/\");\n        return `${year}-${month.padStart(2, \"0\")}-${day.padStart(2, \"0\")}`;\n      },\n      remove(item) {\n        const index = this.friends.indexOf(item.name);\n        if (index >= 0) this.friends.splice(index, 1);\n      },\n      removeTag(item) {\n        const index = this.tags.indexOf(item);\n        if (index >= 0) this.tags.splice(index, 1);\n      },\n    }\n  };\n</script>\n<style lang=\"scss\">\n  .two-line-ellipsis {\n    display: -webkit-box;\n    -webkit-box-orient: vertical;\n    -webkit-line-clamp: 2;\n    overflow: hidden;\n  }\n  .single-line-ellipsis {\n    display: -webkit-box;\n    -webkit-box-orient: vertical;\n    -webkit-line-clamp: 1;\n    overflow: hidden;\n  }\n  // .project_table {\n  //     .v-data-table-header {\n  //         background: #EFF4FF;\n  //         color: #3F4254;\n  //     }\n  // }\n</style>\n"],"sourceRoot":"src/views"}]}