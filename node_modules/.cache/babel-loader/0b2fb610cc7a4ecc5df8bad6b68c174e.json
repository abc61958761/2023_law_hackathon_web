AA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qBACA,KAAA,CAAA,MAAA,CAAA,QAAA,CAAA,sBAAA,CADA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA;AAlCA,CAAA","sourcesContent":["<template>\n  <div>\n    <v-data-table\n      :headers=\"dessertHeaders\"\n      :items=\"purchaseRecords\"\n      :expanded.sync=\"expanded\"\n      hide-default-header\n      item-key=\"purchase.id\"\n      show-expand\n      class=\"elevation-1\"\n    >\n      <template v-slot:item.purchase.date=\"{ item }\">\n        {{ new Date(item.purchase.date).getFullYear() }} /\n        {{ new Date(item.purchase.date).getMonth() }} /\n        {{ new Date(item.purchase.date).getDate() }}\n      </template>\n      <template v-slot:item.purchase.purchaser=\"{ item }\" style=\"background: red\">\n        <v-chip dark>\n          {{ item.purchase.purchaser }}\n        </v-chip>\n      </template>\n      <template v-slot:expanded-item=\"{ headers, item }\">\n        <td :colspan=\"headers.length\">\n          <v-list-item\n            v-for=\"(purchase_record, index) of item.purchase_records\"\n            :key=\"index\"\n          >\n            <v-list-item-content>\n              <v-list-item-title>{{\n                purchase_record.pokemon.name\n              }}</v-list-item-title>\n            </v-list-item-content>\n            <v-list-item-content>\n              <v-list-item-title>\n                <span class=\"mr-10\">\n                  {{ purchase_record.record.count }} 張\n                </span>\n                <span>{{ purchase_record.record.price }} / 張 </span>\n              </v-list-item-title>\n            </v-list-item-content>\n            <v-list-item-content>\n              <v-list-item-title>{{\n                purchase_record.record.count * purchase_record.record.price\n              }}</v-list-item-title>\n            </v-list-item-content>\n          </v-list-item>\n        </td>\n      </template>\n    </v-data-table>\n    <div style=\"position: fixed; bottom: 16px; right: 16px;\">\n      <v-btn class=\"mr-4\" @click=\"cardDialogAction\">新增名稱</v-btn>\n      <v-btn @click=\"itemDialogAction\">新增品項</v-btn>\n    </div>\n    <add-card-dialog\n      :openDialog=\"openCardDialog\"\n      @closeDialog=\"openCardDialog = false\"\n    ></add-card-dialog>\n    <add-item-dialog\n      :openDialog=\"openItemDialog\"\n      @closeDialog=\"openItemDialog = false\"\n    ></add-item-dialog>\n  </div>\n</template>\n<script>\n  import AddCard from \"./dialog/AddCard\";\n  import AddPurchaseItem from \"./dialog/AddPurchaseItem\";\n  import { mapGetters } from \"vuex\";\n\n  export default {\n    data: () => {\n      return {\n        openCardDialog: false,\n        openItemDialog: false,\n        expanded: [],\n        dessertHeaders: [\n          { text: \"購入者\", value: \"purchase.purchaser\" },\n          { text: \"品項名稱\", value: \"purchase.name\" },\n          { text: \"購入時間\", value: \"purchase.date\" },\n          { text: \"購入金額\", value: \"purchase.price\" },\n          { text: \"\", value: \"data-table-expand\" },\n        ],\n      };\n    },\n    components: {\n      \"add-card-dialog\": AddCard,\n      \"add-item-dialog\": AddPurchaseItem,\n    },\n    computed: {\n      ...mapGetters({\n        purchaseRecords: \"purchaseRecords\",\n      }),\n    },\n    methods: {\n      itemDialogAction() {\n        this.openItemDialog = true;\n      },\n      cardDialogAction() {\n        this.openCardDialog = true;\n      },\n    },\n    async mounted() {\n      await this.$store.dispatch(\"queryPurchaseRecords\");\n    },\n  };\n</script>\n<style>\n  .v-data-table\n    .v-data-table__wrapper\n    tbody\n    tr.v-data-table__expanded__content {\n    box-shadow: 0 0;\n    background: rgba(0, 0, 0, 0.1);\n    font-size: 14px;\n  }\n</style>"],"sourceRoot":"src/components"}]}