           dense\n                                    hide-details=\"true\"\n                                    ></v-text-field>\n                            </v-col>\n                        </v-row>\n                        <v-row class=\"mb-4 align-center\" no-gutters>\n                            <v-col md=\"4\">\n                                說明\n                            </v-col>\n                            <v-col md=\"8\">\n                                <v-textarea\n                                auto-grow\n                                outlined\n                                rows=\"1\"\n                                row-height=\"15\"\n                                hide-details=\"true\"\n                                ></v-textarea>\n                            </v-col>\n                        </v-row>\n                    </template>\n\n                    <!-- =============== 日期 ================= -->\n                    <template v-if=\"pattern== '日期'\">\n                        <v-row class=\"mb-4 align-center\" no-gutters>\n                        <v-col md=\"4\">\n                            說明\n                        </v-col>\n                        <v-col md=\"8\">\n                            <v-textarea\n                            auto-grow\n                            outlined\n                            rows=\"1\"\n                            row-height=\"15\"\n                            hide-details=\"true\"\n                            ></v-textarea>\n                        </v-col>\n                        </v-row>\n                    </template>\n                </v-card-text>\n\n                <v-divider></v-divider>\n\n                <v-card-actions>\n                <v-spacer></v-spacer>\n                <v-btn\n                    color=\"#F5F8FA\"\n                    style=\"color:#7E8299\"\n                    depressed\n                    @click=\"markSettingDialog = false\"\n                >\n                    取消\n                </v-btn>\n                <v-btn\n\n                    depressed\n                    color=\"#6291F4\"\n                    style=\"color:#FFFFFF\"\n                    @click=\"markSettingDialog = false\"\n                >\n                    建立\n                </v-btn>\n                </v-card-actions>\n            </v-card>\n        </v-dialog>\n        <v-dialog\n        v-model=\"deleteItem\"\n        width=\"500\"\n        >\n            <v-card>\n                <v-card-title class=\"text-h6\">\n                立即刪除\n                </v-card-title>\n                <v-divider />\n                <v-card-text class=\"py-10\">\n                    <div class=\"font-weight-bold\">提醒您！</div>\n                    <div>刪除此變相之後，無法再復原，必須重新建立</div>\n                </v-card-text>\n\n                <v-divider></v-divider>\n\n                <v-card-actions>\n                <v-spacer></v-spacer>\n                <v-btn\n                    color=\"#F5F8FA\"\n                    style=\"color:#7E8299\"\n                    depressed\n                    @click=\"deleteItem = false\"\n                >\n                    取消\n                </v-btn>\n                <v-btn\n\n                    depressed\n                    color=\"#F1416C\"\n                    style=\"color:#FFFFFF\"\n                    @click=\"clickDeleteItem\"\n                >\n                    刪除\n                </v-btn>\n                </v-card-actions>\n            </v-card>\n        </v-dialog>\n    </div>\n    \n</template>\n<script>\nexport default {\n    data() {\n        return {\n            multipleItems: [{\n                value: \"\"\n            }],\n            checkbox: true,\n            switch1: true,\n            selfType: \"有無變相\",\n            selfTypeItems: [{\n                text: \"有無變相\"\n            }, {\n                text: \"連續變相\"\n            }],\n            pattern: \"多選\",\n            patternItems:[{\n                text: \"多選\"\n            }, {\n                text: \"文字欄位\"\n            }, {\n                text: \"日期\"\n            }],\n            markSettingDialog: false,\n            deleteItem: false,\n            radioGroup: \"\",\n            items: [\n                {\n                text: '專案列表',\n                disabled: false,\n                to: \"/projects\",\n                exact: true\n                },\n                {\n                text: '標記設定',\n                disabled: true\n                },\n            ],\n            test: [{\n                type: 'radio',\n                value: '2'\n            }, {\n                type: 'textarea',\n                value: '2'\n            }, {\n                type: 'field',\n                value: '2'\n            }]\n        }\n    },\n    methods: {\n        clickDeleteItem() {\n            this.deleteItem = false\n        },\n        addMultipleItems() {\n            this.multipleItems.push({value: \"\"})\n        },\n        removeMultipleItems(index) {\n            this.multipleItems.splice(index, 1);\n        },\n        down(index) {\n            const tmp = this.test[index]\n            this.test.splice(index, 1)\n            this.test.splice(index-1, 0, tmp)\n        },\n        top(index) {\n            const tmp = this.test[index]\n            this.test.splice(index, 1)\n            this.test.splice(index+1, 0, tmp)\n        }\n    }\n}\n</script>"],"sourceRoot":"src/views"}]}