BA,EAAA,OAAA,EAAA;AACA,IAAA,gBADA,8BACA;AACA,WAAA,cAAA,GAAA,IAAA;AACA,KAHA;AAIA,IAAA,gBAJA,8BAIA;AACA,WAAA,cAAA,GAAA,IAAA;AACA,KANA;AAOA,IAAA,UAPA,sBAOA,EAPA,EAOA,KAPA,EAOA;AACA,UAAA,KAAA,EAAA;AACA,aAAA,eAAA,CAAA,GAAA,CAAA,EAAA;AACA,OAFA,MAEA;AACA,aAAA,eAAA,CAAA,MAAA,CAAA,EAAA;AACA;;AACA,WAAA,YAAA;AACA,KAdA;AAeA,IAAA,iBAfA,+BAeA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,KAAA,CAAA,MAAA,CAAA,QAAA,CAAA,uBAAA,EAAA;AACA,kBAAA,GAAA,EAAA,KAAA,CAAA,IAAA,CAAA,KAAA,CAAA,eAAA;AADA,iBAAA,CADA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA,KAnBA;AAoBA,IAAA,aApBA,2BAoBA,CAAA;AApBA,GA5BA;AAkDA,EAAA,OAlDA,qBAkDA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qBACA,MAAA,CAAA,MAAA,CAAA,QAAA,CAAA,sBAAA,CADA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA;AApDA,CAAA","sourcesContent":["<template>\n  <div\n    class=\"d-flex flex-column flex-grow-1\"\n    style=\"height: 100%;\n        overflow: hidden;\"\n  >\n    <h3>買入紀錄</h3>\n\n    <v-list style=\"height: 100%;overflow: scroll;\">\n      <v-list-group v-for=\"item of purchaseRecords\" :key=\"item.purchase.id\">\n        <template v-slot:activator>\n          <v-list-item-content style=\"max-width: 50px\">\n            <v-checkbox\n              class=\"ma-0 pa-0 ml-2\"\n              hide-details\n              v-model=\"item.isDelete\"\n              @change=\"deleteItem(item.purchase.id, item.isDelete)\"\n            ></v-checkbox>\n          </v-list-item-content>\n          <v-list-item-content style=\"display: block\">\n            <v-chip small dark class=\"mr-2\" :class=\"[item.purchase.purchaser]\">\n              {{ item.purchase.purchaser }}\n            </v-chip>\n            <v-chip\n              small\n              dark\n              :class=\"[item.purchase.split === 'true' ? split : unsplit]\"\n            >\n              <span v-if=\"item.purchase.split === 'true'\">已拆帳</span>\n              <span v-else>未拆帳</span>\n            </v-chip>\n          </v-list-item-content>\n          <v-list-item-content>\n            {{ new Date(item.purchase.date).getFullYear() }} /\n            {{ new Date(item.purchase.date).getMonth() + 1 }} /\n            {{ new Date(item.purchase.date).getDate() }}\n          </v-list-item-content>\n          <v-list-item-content\n            >＄{{ item.purchase.total_price }}</v-list-item-content\n          >\n        </template>\n\n        <v-list-item\n          v-for=\"(purchase_record, index) of item.purchase_records\"\n          :key=\"index\"\n        >\n          <v-list-item-content style=\"max-width: 50px\"> </v-list-item-content>\n          <v-list-item-content></v-list-item-content>\n          <v-list-item-content>\n            <div>{{ purchase_record.pokemon.name }}</div>\n          </v-list-item-content>\n          <v-list-item-content>\n            <div>\n              <span class=\"mr-10\">{{ purchase_record.record.count }} 張</span>\n              <span\n                >{{\n                  purchase_record.record.total_price /\n                    purchase_record.record.count\n                }}\n                / 張\n              </span>\n            </div>\n          </v-list-item-content>\n          <v-list-item-content>\n            <div>＄{{ purchase_record.record.total_price }}</div>\n          </v-list-item-content>\n        </v-list-item>\n      </v-list-group>\n    </v-list>\n\n    <div class=\"mb-1 d-flex\">\n      <v-btn @click=\"deleteItemsAction\" :disabled=\"tempDeleteItems.size == 0\"\n        >刪除</v-btn\n      >\n      <v-spacer></v-spacer>\n      <input type=\"file\" @change=\"uploadPokemon\" />\n      <v-btn class=\"mr-4\" @click=\"cardDialogAction\">新增名稱</v-btn>\n      <v-btn @click=\"itemDialogAction\">新增品項</v-btn>\n    </div>\n    <add-card-dialog\n      :openDialog=\"openCardDialog\"\n      @closeDialog=\"openCardDialog = false\"\n    ></add-card-dialog>\n    <add-item-dialog\n      :openDialog=\"openItemDialog\"\n      @closeDialog=\"openItemDialog = false\"\n    ></add-item-dialog>\n  </div>\n</template>\n<script>\n  import AddCard from \"../components/dialog/AddCard\";\n  import AddPurchaseItem from \"../components/dialog/AddPurchaseItem\";\n  import { mapGetters } from \"vuex\";\n\n  export default {\n    data: () => {\n      return {\n        openCardDialog: false,\n        openItemDialog: false,\n        expanded: [],\n        dessertHeaders: [\n          { text: \"刪除\", value: \"delete\" },\n          { text: \"購入者\", value: \"purchase.purchaser\" },\n          { text: \"品項名稱\", value: \"purchase.name\" },\n          { text: \"購入時間\", value: \"purchase.date\" },\n          { text: \"購入金額\", value: \"purchase.total_price\" },\n          { text: \"\", value: \"data-table-expand\" },\n        ],\n        tempDeleteItems: new Set(),\n        split: \"split\",\n        unsplit: \"unsplit\",\n      };\n    },\n    components: {\n      \"add-card-dialog\": AddCard,\n      \"add-item-dialog\": AddPurchaseItem,\n    },\n    computed: {\n      ...mapGetters({\n        purchaseRecords: \"purchaseRecords\",\n      }),\n    },\n    methods: {\n      itemDialogAction() {\n        this.openItemDialog = true;\n      },\n      cardDialogAction() {\n        this.openCardDialog = true;\n      },\n      deleteItem(id, value) {\n        if (value) {\n          this.tempDeleteItems.add(id);\n        } else {\n          this.tempDeleteItems.delete(id);\n        }\n        this.$forceUpdate();\n      },\n      async deleteItemsAction() {\n        await this.$store.dispatch(\"deletePurchaseRecords\", {\n          ids: Array.from(this.tempDeleteItems),\n        });\n      },\n      uploadPokemon() {},\n    },\n    async mounted() {\n      await this.$store.dispatch(\"queryPurchaseRecords\");\n    },\n  };\n</script>\n<style lang=\"scss\" scoped>\n  .v-chip {\n    &.Chad {\n      background: #80b2ff;\n    }\n    &.Carol {\n      background: #edacea;\n    }\n    &.split {\n      background: #56e18e;\n    }\n    &.unsplit {\n      background: #ffd08a;\n    }\n  }\n</style>\n"],"sourceRoot":"src/views"}]}