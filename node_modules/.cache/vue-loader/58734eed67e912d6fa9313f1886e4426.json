      class=\"mr-2\"\n          ></v-text-field>\n          <v-text-field\n            label=\"標號\"\n            v-model=\"grade\"\n            hide-details\n            outlined\n          ></v-text-field>\n        </div>\n      </v-card-text>\n      <v-card-actions>\n        <v-spacer></v-spacer>\n        <v-btn outlined @click=\"close\">取消</v-btn>\n        <v-btn depressed @click=\"addCardAction\">新增</v-btn>\n      </v-card-actions>\n    </v-card>\n  </v-dialog>\n</template>\n<script>\n  export default {\n    props: [\"openDialog\"],\n    data: () => {\n      return {\n        pokemonName: \"\",\n        grade: \"\",\n        number: \"\",\n        series: \"\",\n      };\n    },\n    methods: {\n      async addCardAction() {\n        await this.$store.dispatch(\"createPokemon\", {\n          name: `${this.series}_${this.number} - ${this.pokemonName} - ${this.grade}`,\n        });\n        this.close();\n      },\n      close() {\n        this.clean();\n        this.$emit(\"closeDialog\");\n      },\n      clean() {\n        this.pokemonName = \"\";\n        this.grade = \"\";\n        this.number = \"\";\n        this.series = \"\";\n      },\n    },\n  };\n</script>\n"]}]}